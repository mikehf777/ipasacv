<table class="table table-striped">
  <thead>
    <tr>
      <th>Id</th>
      <th>Url</th>
      <th>Fecha de subida</th>
      <th>Opciones</th>
    </tr>
  </thead>
  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= link_to video.id, video_path(video) %></td>
        <td><%= video.url %></td>
        <td><%= video.created_at %></td>
        <td>
          <%= link_to_edit(admin_video_path(video)) %>
          <%= link_to_delete(admin_video_path(video)) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>